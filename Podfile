  pod 'RxSwift',    '~> 3.0'
  pod 'RxCocoa',    '~> 3.0'
  pod 'SnapKit', '3.0.0'
  pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift3'
  pod 'PopupDialog', '~> 0.5.0'
  pod 'RealmSwift', '2.8.2'
  pod 'SlideMenuControllerSwift'

end

target 'Pairing' do
